{
    "multipart": [
		{ "when": { "has_record": "false" }, 
			"apply": [
				{ "model": "block/jukebox" },
				{ "model": "block/jukebox", "y": 90 },
				{ "model": "block/jukebox", "y": 180 },
				{ "model": "block/jukebox", "y": 270 }
			]
		},
		{ "when": { "has_record": "true" },
			"apply": [
				{ "model": "block/jukebox_disc" },
				{ "model": "block/jukebox_disc", "y": 90 },
				{ "model": "block/jukebox_disc", "y": 180 },
				{ "model": "block/jukebox_disc", "y": 270 }
			]
        }
    ]
}
